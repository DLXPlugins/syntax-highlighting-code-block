{
    "name": "westonruter/code-syntax-block",
    "description": "A WordPress plugin which extends Gutenberg adding color syntax highlighting to the code block.",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "require": {
        "scrivo/highlight.php": "v9.*"
    },
    "require-dev": {
        "wp-cli/i18n-command": "^2.1"
    },
    "scripts": {
      "make-pot": "wp i18n make-pot . languages/code-syntax-block.pot --include=code-syntax-block.js"
    }
}
